<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="org.model.Shopingorderitem" table="shopingorderitem" schema="devicemanagefzl">
        <id name="shopingOrderItemId">
            <column name="ShopingOrderItemID" sql-type="int(11)"/>
            <generator class="native"></generator>
        </id>
        <property name="buyNum">
            <column name="BuyNum" sql-type="int(11)" not-null="true"/>
        </property>
        <many-to-one name="shopingorder" class="org.model.Shopingorder" lazy="false" fetch="join">
            <column name="ShopingOrderID"/>
        </many-to-one>
        <many-to-one name="device" class="org.model.Device" lazy="false" fetch="join">
            <column name="DeviceID"/>
        </many-to-one>
    </class>
</hibernate-mapping>